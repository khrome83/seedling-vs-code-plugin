{
  "name": "seedling",
  "displayName": "Seedling",
  "description": "Syntax Highlighter for Seedling Template Language",
  "version": "1.0.1",
  "publisher": "Seedling",
  "icon": "icons/logo.png",
  "galleryBanner": {
    "color": "#B4C86B"
  },
  "repository": {
    "url": "https://github.com/use-seedling/seedling-vs-code-plugin",
    "type": "GIT"
  },
  "engines": {
    "vscode": "^1.48.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "seedling",
        "aliases": [
          "Seedling",
          "seedling"
        ],
        "extensions": [
          ".seed",
          ".html"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "seedling",
        "scopeName": "source.seedling",
        "path": "./syntaxes/seedling.tmLanguage.json"
      }
    ]
  }
}
